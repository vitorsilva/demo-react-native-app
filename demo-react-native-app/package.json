    "@types/react": "~19.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.12",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npx eslint --fix --config demo-react-native-app/eslint.config.js",
      "npx prettier --write --config demo-react-native-app/.prettierrc"
